-- Criação das tabelas
CREATE TABLE agencia (
    nome_agencia TEXT NOT NULL PRIMARY KEY,
    cidade_agencia TEXT,
    ativo_agencia REAL
);

CREATE TABLE cliente (
    nome_cliente TEXT NOT NULL PRIMARY KEY,
    rua_cliente TEXT NOT NULL,
    cidade_cliente TEXT NOT NULL
);

CREATE TABLE conta (
    numero_conta INTEGER NOT NULL,
    nome_agencia TEXT NOT NULL,
    nome_cliente TEXT NOT NULL,
    saldo_conta INTEGER,
    PRIMARY KEY (numero_conta, nome_agencia, nome_cliente),
    FOREIGN KEY (nome_agencia) REFERENCES agencia(nome_agencia),
    FOREIGN KEY (nome_cliente) REFERENCES cliente(nome_cliente)
);

CREATE TABLE deposito (
    numero_deposito INTEGER NOT NULL PRIMARY KEY,
    numero_conta INTEGER NOT NULL,
    nome_agencia TEXT NOT NULL,
    nome_cliente TEXT NOT NULL,
    saldo_deposito REAL NOT NULL,
    FOREIGN KEY (numero_conta, nome_agencia, nome_cliente) REFERENCES conta(numero_conta, nome_agencia, nome_cliente)
);

CREATE TABLE emprestimo (
    numero_emprestimo INTEGER NOT NULL PRIMARY KEY,
    nome_cliente TEXT NOT NULL,
    numero_conta INTEGER NOT NULL,
    nome_agencia TEXT NOT NULL,
    valor_emprestimo REAL,
    juros_emprestimo INTEGER,
    data_emprestimo TEXT,
    FOREIGN KEY (numero_conta, nome_agencia, nome_cliente) REFERENCES conta(numero_conta, nome_agencia, nome_cliente)
);

